        {
            "name": "classifer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/jetson/classifier.py",
            "args": [
                "--model_dir", "${workspaceFolder}/jetson/models/hand_tf/",
                "--output_dir", "${workspaceFolder}/jetson/models/hand_tf/output/"
            ]
        }
    ]
}